{
  "name": "ng-utilities",
  "displayName": "ng-utilities",
  "description": "angular utilities from makeflow",
  "version": "0.0.1",
  "publisher": "mf",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.switchSameNameFile",
        "title": "Switch same name file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "tslint": "^5.3.2",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "vts": "^5.3.2"
  },
  "dependencies": {
    "villa": "^0.2.11"
  }
}
